<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.commit.mappers.FamilyMapper">

	<!-- 가족정보 select문 -->
	<select id="familySelect" parameterType="MypageVo" resultType="MapageVo">
	SELECT
		f.mother,
	    f.father,
	    f.child, 
		f.sibling
	FROM user u
	JOIN family f ON u.id = f.user_id
	WHERE u.id = #{userId}
	</select>
	 
</mapper>	